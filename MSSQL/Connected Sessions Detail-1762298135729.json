          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \r\nt2.Upn, \r\nt2.UserName,\r\nt2.Domain, \r\nt1.ClientName, \r\nt1.ClientAddress, \r\nt1.ClientVersion, \r\nt1.ClientPlatform, \r\nt3.DnsName, \r\nt3.IpAddress, \r\nt3.AgentVersion, \r\nt3.IsInMaintenanceMode, \r\nt3.OSType, \r\nt1.ConnectedViaHostName, \r\nt1.ConnectedViaIPAddress, \r\nt1.LaunchedViaHostName, \r\nt1.LaunchedViaIPAddress, \r\nt1.IsReconnect, \r\nt1.IsSecureIca, \r\nt1.Protocol, \r\nt1.LogOnStartDate, \r\nt1.LogOnEndDate,\r\nDATEDIFF(SECOND, t1.LogOnStartDate, t1.LogOnEndDate) AS LogOnElapsedSeconds, \r\nt1.BrokeringDuration, \r\nt1.BrokeringDate, \r\nt1.ClientSessionValidateDate, \r\nt1.ServerSessionValidateDate, \r\nt1.EstablishmentDate, \r\nt1.HdxStartDate, \r\nt1.HdxEndDate,\r\nDATEDIFF(SECOND, t1.HdxStartDate, t1.HdxEndDate) AS HdxElapsedSeconds,\r\nt1.AuthenticationDuration,\r\nCAST(t1.AuthenticationDuration AS FLOAT) / 1000 AS AuthenticationSeconds,\r\nt1.GpoStartDate, \r\nt1.GpoEndDate,\r\nDATEDIFF(SECOND, t1.GpoStartDate, t1.GpoEndDate) AS GpoElapsedSeconds,\r\nt1.LogOnScriptsStartDate, \r\nt1.LogOnScriptsEndDate,\r\nDATEDIFF(SECOND, t1.LogOnScriptsStartDate, t1.LogOnScriptsEndDate) AS LogOnScriptsElapsedSeconds,\r\nt1.ProfileLoadStartDate, \r\nt1.ProfileLoadEndDate, \r\nDATEDIFF(SECOND, t1.ProfileLoadStartDate, t1.ProfileLoadEndDate) AS ProfileLoadElapsedSeconds,\r\nt1.InteractiveStartDate, \r\nt1.InteractiveEndDate, \r\nDATEDIFF(SECOND, t1.InteractiveStartDate, t1.InteractiveEndDate) AS InteractiveElapsedSeconds,\r\nt2.ID\r\nFROM (\r\n    SELECT SessionKey, UserId, MachineId\r\n    FROM MonitorData.Session\r\n    WHERE ConnectionState = 5\r\n) AS subquery\r\n    LEFT JOIN MonitorData.Connection  AS t1 ON subquery.SessionKey = t1.SessionKey\r\n    LEFT JOIN [MonitorData].[User] AS t2 ON subquery.UserId = t2.ID\r\n    LEFT JOIN [MonitorData].[Machine] AS t3 ON subquery.MachineId = t3.Id\r\nWHERE t2.UserName IN (${ConnectedUsers});",
